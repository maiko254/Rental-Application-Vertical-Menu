            <HBox layoutX="10.0" layoutY="355.0" prefHeight="31.0" prefWidth="600.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="134.0" text="Lease-End-Date" />
                  <JFXDatePicker fx:id="leaseEndDate" prefHeight="28.0" prefWidth="218.0" />
               </children>
            </HBox>
