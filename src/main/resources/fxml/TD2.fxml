                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </HBox>
                  <TitledPane fx:id="houseComboTitledPane" expanded="false" prefHeight="119.0" prefWidth="218.0" stylesheets="@../styles/newCascadeStyleSheet.css" text="Select House" textFill="#fdfdfd">
