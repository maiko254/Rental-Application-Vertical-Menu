<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="searchPane" prefHeight="424.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox fx:id="searchHbox" prefHeight="28.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER">
          <BorderPane.margin>
              <Insets />
          </BorderPane.margin>
         <children>
            <TextField fx:id="searchText" prefHeight="31.0" prefWidth="330.0" />
            <Button fx:id="clearButton" mnemonicParsing="false" prefHeight="31.0" prefWidth="102.0" text="Clear Filters" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <TableView fx:id="searchTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="houseNoCol" prefWidth="121.0" text="House Number" />
          <TableColumn fx:id="tenantNameCol" minWidth="0.0" prefWidth="158.0" text="Tenant Name" />
        </columns>
      </TableView>
   </center>
</BorderPane>
